.char {
    float: left;
    margin: 1px 21px 0 -19px !important;
}

i {
    font-style: normal; /* s e m a n t i c s */
}

li li {
    line-height: 1em;

    .skill {
        display: inline-block;
    }
    .skill::after {
        content: " |";
        color: #666;
    }

    .skill:last-child::after {
        content: none;
    }
}
